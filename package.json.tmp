{
  "name": "citadao-devenv",
  "version": "0.1.0",
  "description": "Citadao env",
  "repository": "https://github.com/nickreynolds/citadao.git",
  "private": true,
  "contributors": [
    "Andrew Spiering",
    "Nick Reynolds"
  ],
  "license": "MIT",
  "devDependencies": {},
  "scripts": {
    "postinstall": "cross-os postinstallInternal",
    "postinstallInternal": {
      "darwin": "local_modules install --force --dir src",
      "win32": "local_modules.cmd install --force --dir src",
      "linux": "local_modules install --force --dir src"
    },
    "localmod": "cross-os localmodInternal",
    "localmodInternal": {
      "darwin": "local_modules link --force --dir src",
      "win32": "local_modules.cmd link --force --dir src",
      "linux": "local_modules link --force --dir src"
    },
    "testrpc": "( cd src/app-contracts && yarn testrpc )"
  },
  "dependencies": {
    "cross-os": "^1.1.2",
    "local_modules": "^1.0.8"
  },
  "main": "index.js",
  "workspaces": [
    "src/*"
  ]
}
