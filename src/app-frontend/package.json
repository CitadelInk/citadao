{
  "name": "app-frontend",
  "version": "0.1.0",
  "description": "Frontend for CitaDao",
  "main": "index.js",
  "repository": "https://github.com/Charterhouse/TruffleDevEnv.git",
  "contributors": [
    "Andrew Spiering",
    "Nick Reynolds"
  ],
  "license": "MIT",
  "scripts": {
    "config": "cross-os configInternal",
    "configInternal": {
      "darwin": "echo {'\"Server\"':$(ip route get 1 | awk '{print $NF;exit}' | sed -e 's/^/\"/' | sed -e 's/$/\"/')} > ./app/config.json",
      "win32": "FIX ME",
      "linux": "echo {'\"Server\"':$(ip route get 1 | awk '{print $NF;exit}' | sed -e 's/^/\"/' | sed -e 's/$/\"/')} > ./app/config.json"
    },
    "start": "yarn run config && webpack-dev-server --hot --history-api-fallback --open --host 0.0.0.0",
    "test": "mocha  --compilers js:babel-register",
    "test:watch": "yarn run test -- --reporter min --watch"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "truffle-contract": "^2.0.4",
    "cross-os": "^1.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
