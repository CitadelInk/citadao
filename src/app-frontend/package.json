  "scripts": {
    "config": "cross-os configInternal",
    "configInternal": {
      "darwin": "echo {'\"Server\"':$(ip route get 1 | awk '{print $NF;exit}' | sed -e 's/^/\"/' | sed -e 's/$/\"/')} > ./app/config.json",
      "win32": "FIX ME",
      "linux": "echo {'\"Server\"':$(ip route get 1 | awk '{print $NF;exit}' | sed -e 's/^/\"/' | sed -e 's/$/\"/')} > ./app/config.json"
    },
    "start": "yarn run config && webpack-dev-server --hot --history-api-fallback --open --host 0.0.0.0",
    "build": "yarn run config && rm -rf public/** && NODE_ENV=production webpack -p --progress --optimize-minimize",
    "test": "mocha  --compilers js:babel-register",
    "test:watch": "yarn run test -- --reporter min --watch"
  },
  "dependencies": {
    "ampersand-router": "^5.0.0",
