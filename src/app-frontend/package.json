{
  "name": "app-frontend",
  "version": "0.1.0",
  "description": "Frontend for CitaDao",
  "main": "index.js",
  "repository": "https://github.com/Charterhouse/TruffleDevEnv.git",
  "contributors": [
    "Andrew Spiering",
    "Nick Reynolds"
  ],
  "license": "MIT",
  "scripts": {
    "config": "cross-os configInternal",
    "configInternal": {
      "darwin": "echo {'\"Server\"':$(ip route get 1 | awk '{print $NF;exit}' | sed -e 's/^/\"/' | sed -e 's/$/\"/')} > ./app/config.json",
      "win32": "FIX ME",
      "linux": "echo {'\"Server\"':$(ip route get 1 | awk '{print $NF;exit}' | sed -e 's/^/\"/' | sed -e 's/$/\"/')} > ./app/config.json"
    },
    "start": "yarn run config && webpack-dev-server --hot --history-api-fallback --open --host 0.0.0.0",
    "build": "yarn run config && rm -rf public/** && NODE_ENV=production webpack -p --progress",
    "test": "mocha  --compilers js:babel-register",
    "test:watch": "yarn run test -- --reporter min --watch"
  },
  "dependencies": {
    "ampersand-router": "^5.0.0",
    "compression": "^1.6.2",
    "compression-webpack-plugin": "^0.3.2",
    "cross-os": "^1.1.2",
    "immutable": "^3.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.5",
    "react-scroll": "^1.5.4",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "truffle-contract": "^2.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^2.26.0",
    "json-loader": "^0.5.7",
    "react-hot-loader": "^3.0.0-beta.6",
    "truffle-solidity-loader": "git+https://github.com/sogoiii/truffle-solidity-loader.git#1f1e213d52f033b6863218307b8968ae68220fe1",
    "webpack": "^2.2.1",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.4.1"
  }
}
